PREREQ=""
prereqs()
{
        echo "$PREREQ"
}
case $1 in
prereqs)
        prereqs
        exit 0
        ;;
esac

sysctl -p ${rootmnt}/etc/sysctl.conf
sysctl -p ${rootmnt}/etc/sysctl.d/*.conf

true
